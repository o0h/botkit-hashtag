{
  parser: "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  plugins: ["import", "prettier"],
  "extends": [
    "eslint:recommended",
    'plugin:import/errors',
    "plugin:prettier/recommended",
    "prettier",
  ],
  "rules": {
    "prettier/prettier": [
      2,
      {
        printWidth: 120,
        "semi": false,
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
